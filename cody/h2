#include <stdio.h>

int main() {
    int arr[] = {3, 1, 3, 2, 1, 3};
    int length = 6;
    int arrRes[length];
    
    for (int i = 1; i <= length; i++) {
        if (arr[i - 1] > arr[i]) {
            arrRes[i - 1] = arr[i];
            arrRes[i] = arr[i - 1];
        };
    };
    
    for (int i = 0; i < length; i++) {
        printf("%d, ", arrRes[i]);
    };

    
    return 0;
}