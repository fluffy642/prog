#include <stdio.h>

int main() {
    int arr[] = {};
    int length = 5;
    
    int num;
    for (int i = 0; ; i++) {
        printf("Napis cislo(napis 0 pro ukonceni): ");
        scanf("%d", &num);
        if (num == 0) {
            length = i;
            break;
        }
        arr[i] = num;
    };
    
    for (int i = 0; i <= length; i++) {
        printf("Cislo 1: %d", arr[i]);
    };
    

    return 0;
}